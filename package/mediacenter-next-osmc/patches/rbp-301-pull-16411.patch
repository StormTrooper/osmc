From 07bdf6d9c56e7bf8522ca625f8864cdd778464ad Mon Sep 17 00:00:00 2001
From: Alwin Esch <alwin.esch@web.de>
Date: Sun, 28 Jul 2019 12:20:27 +0200
Subject: [PATCH] [addons] fix binary addon lib name for iOS

---
 xbmc/addons/binary-addons/BinaryAddonType.cpp | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/xbmc/addons/binary-addons/BinaryAddonType.cpp b/xbmc/addons/binary-addons/BinaryAddonType.cpp
index fa5478010351..0b13c15c4b0d 100644
--- a/xbmc/addons/binary-addons/BinaryAddonType.cpp
+++ b/xbmc/addons/binary-addons/BinaryAddonType.cpp
@@ -66,11 +66,11 @@ const char* CBinaryAddonType::GetPlatformLibraryName(const TiXmlElement* element
 #elif defined(TARGET_WINDOWS_STORE)
   libraryName = element->Attribute("library_windowsstore");
 #elif defined(TARGET_DARWIN)
-#if defined(TARGET_DARWIN_IOS)
-  libraryName = element->Attribute("library_ios");
-  if (libraryName == nullptr)
-#endif
+#if defined(TARGET_DARWIN_EMBEDDED)
+  libraryName = element->Attribute("library_darwin_embedded");
+#else
   libraryName = element->Attribute("library_osx");
+#endif
 #endif
 
   return libraryName;
