From cef4afd018525143487924c4f980c8a07d3a9876 Mon Sep 17 00:00:00 2001
From: howie-f <rftc@gmx.de>
Date: Sat, 25 Apr 2020 10:01:11 +0200
Subject: [PATCH] [Subtitles] fix smi subtitles to use quoted start tags

---
 xbmc/cores/VideoPlayer/DVDSubtitles/DVDSubtitleParserSami.cpp | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/xbmc/cores/VideoPlayer/DVDSubtitles/DVDSubtitleParserSami.cpp b/xbmc/cores/VideoPlayer/DVDSubtitles/DVDSubtitleParserSami.cpp
index 2f186bed3f8d..e103b3bbac23 100644
--- a/xbmc/cores/VideoPlayer/DVDSubtitles/DVDSubtitleParserSami.cpp
+++ b/xbmc/cores/VideoPlayer/DVDSubtitles/DVDSubtitleParserSami.cpp
@@ -35,7 +35,7 @@ bool CDVDSubtitleParserSami::Open(CDVDStreamInfo &hints)
   char line[1024];
 
   CRegExp reg(true);
-  if (!reg.RegComp("<SYNC START=([0-9]+)>"))
+  if (!reg.RegComp("<SYNC START=\"?([0-9]+)\"?>"))
     return false;
 
   std::string strFileName;
